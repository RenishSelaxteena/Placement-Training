import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        int x1 = sc.nextInt();
        int y1 = sc.nextInt();
        int x2 = sc.nextInt();
        int y2 = sc.nextInt();
        
        float z1 = (x1+x2)/2.0f;
        float z2 = (y1+y2)/2.0f;
        
        System.out.printf("Arun's house is located at(%.1f,%.1f)", z1, z2);
        
    }
}
